{
  "compilerOptions": {
    "target": "es2016",                                
    "module": "ESNext",                   
    "esModuleInterop": true,                                                  
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                      
    "skipLibCheck": true,  
    "moduleResolution":"node",
    "outDir": "./types",                 
  },
  "include": ["**/*.ts","./*.ts"],
  "exclude": ["node_modules"]
}
