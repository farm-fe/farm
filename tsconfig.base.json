{
  "compilerOptions": {
    "lib": ["DOM", "ES2020"],
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2020",
    "module": "node16",
    "moduleResolution": "node16",
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
