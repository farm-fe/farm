      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    },
    "./package.json": "./package.json"
  },
  "napi": {
    "name": "farm",
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
